{
	"luau-lsp.completion.imports.enabled": true,
	"luau-lsp.completion.imports.suggestServices": true,
	"luau-lsp.completion.imports.suggestRequires": true,
	"luau-lsp.completion.autocompleteEnd": true,
	"luau-lsp.sourcemap.rojoProjectFile": "serve.project.json",
	"[luau]": {
		"editor.defaultFormatter": "JohnnyMorganz.stylua"
	},
	"files.eol": "\n",
	"luau-lsp.sourcemap.generatorCommand": "rojo sourcemap serve.project.json -o sourcemap.json --watch",
	"luau-lsp.sourcemap.sourcemapFile": "sourcemap.json",
	"luau-lsp.fflags.enableNewSolver": true,
	"luau-lsp.fflags.sync": true,
	"luau-lsp.completion.imports.stringRequires.enabled": true
}
