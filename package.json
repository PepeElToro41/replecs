  "devDependencies": {
    "@rbxts/compiler-types": "^2.3.0-types.1",
    "@rbxts/types": "^1.0.768",
    "roblox-ts": "3.0.0",
    "typescript": "=5.5.3",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "@rbxts/jecs": "^0.9.0"
  }
